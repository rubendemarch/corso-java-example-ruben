<!-- QUESTO E' IL FILE CHE COMPONE LE QUERY -->
<mapper namespace="key">
	<select 
		id="lastInsertedKey"
		resultType="String"
		parameterType="HashMap">
		select max(${colName}) from ${tableName}
	</select>
</mapper>